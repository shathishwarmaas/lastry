pipeline {
    agent any

    stages {
        stage('GIT PULL') {
            steps {
                git branch: "love", url: 'https://github.com/shathishwarmaas/lastry.git'
            }
        }
        stage('TEST') {
            steps {
                 sh '''
                  #!/bin/sh
                  flutter test
                  '''
            }
        }
        stage('BUILD') {
            steps {
                sh '''
                  #!/bin/sh
                  flutter build apk --debug
                  '''
            }
        }
       
    }
}
